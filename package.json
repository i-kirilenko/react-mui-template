    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/babel__core": "^7.1.19",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.16.0",
    "c8": "^7.11.3",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "firebase-tools": "^11.14.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-order-config-standard": "^0.1.3",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.16.0"
  },
  "license": "MIT",
  "name": "react-mui-template",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-kirilenko/react-mui-template.git"
  },
  "scripts": {
    "dev": "vite --port 3333",
    "lint:styles": "stylelint --fix 'src/**/**.{css,style.js,style.ts}'",
    "lint:scripts": "eslint --fix 'src/**/**.{ts,tsx}'",
    "format": "yarn lint:styles && yarn lint:scripts && scripts/postformat.sh",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vitest --run --coverage",
    "build:dev": "tsc && vite build --mode=dev",
    "build:stage": "tsc && vite build --mode=stage",
    "build:master": "tsc && vite build",
    "preview": "vite preview",
    "deploy:dev": "yarn build:dev && firebase deploy --only hosting:dev",
    "deploy:stage": "yarn build:stage && firebase deploy --only hosting:stage",
    "deploy:master": "yarn build:master && firebase deploy --only hosting:master"
  },
  "version": "0.0.0"
}
